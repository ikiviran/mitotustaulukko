{
  "db": {
    "connectionString": "{{DATABASE_URL}}",
    "ssl": {
      "rejectUnauthorized": false
    }
  },
  "outDir": "./src",
  "progressListener": true,
  "warningListener": true,
  "schemas": {
    "public": {
      "include": "*",
      "exclude": ["schema_migrations"]
    }
  },
  "schemaJSDoc": true
}
